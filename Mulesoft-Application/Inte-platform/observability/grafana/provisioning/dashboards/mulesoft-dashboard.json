{
    "id": null,
    "uid": "mulesoft-main",
    "title": "MuleSoft Anypoint Platform",
    "tags": [
        "mulesoft",
        "integration"
    ],
    "timezone": "browser",
    "refresh": "10s",
    "time": {
        "from": "now-1h",
        "to": "now"
    },
    "panels": [
        {
            "id": 1,
            "title": "Active Integrations",
            "type": "stat",
            "gridPos": {
                "x": 0,
                "y": 0,
                "w": 4,
                "h": 4
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "active_integrations_count",
                    "refId": "A"
                }
            ],
            "options": {
                "colorMode": "value",
                "graphMode": "area"
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 2,
            "title": "Total Executions",
            "type": "stat",
            "gridPos": {
                "x": 4,
                "y": 0,
                "w": 4,
                "h": 4
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "sum(integration_executions_total)",
                    "refId": "A"
                }
            ],
            "options": {
                "colorMode": "value",
                "graphMode": "area"
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "blue",
                                "value": null
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 3,
            "title": "Success Rate",
            "type": "gauge",
            "gridPos": {
                "x": 8,
                "y": 0,
                "w": 4,
                "h": 4
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "sum(integration_executions_total{status='success'}) / sum(integration_executions_total) * 100",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "min": 0,
                    "max": 100,
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "red",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 80
                            },
                            {
                                "color": "green",
                                "value": 95
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 4,
            "title": "Total Errors",
            "type": "stat",
            "gridPos": {
                "x": 12,
                "y": 0,
                "w": 4,
                "h": 4
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "sum(integration_errors_total)",
                    "refId": "A"
                }
            ],
            "options": {
                "colorMode": "value",
                "graphMode": "area"
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 5
                            },
                            {
                                "color": "red",
                                "value": 10
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 5,
            "title": "Records Processed",
            "type": "stat",
            "gridPos": {
                "x": 16,
                "y": 0,
                "w": 4,
                "h": 4
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "sum(integration_records_processed_total)",
                    "refId": "A"
                }
            ],
            "options": {
                "colorMode": "value",
                "graphMode": "area"
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "purple",
                                "value": null
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 6,
            "title": "API Calls",
            "type": "stat",
            "gridPos": {
                "x": 20,
                "y": 0,
                "w": 4,
                "h": 4
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "sum(integration_api_calls_total)",
                    "refId": "A"
                }
            ],
            "options": {
                "colorMode": "value",
                "graphMode": "area"
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "orange",
                                "value": null
                            }
                        ]
                    }
                }
            }
        },
        {
            "id": 7,
            "title": "Integration Status",
            "type": "table",
            "gridPos": {
                "x": 0,
                "y": 4,
                "w": 8,
                "h": 6
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "integration_status",
                    "format": "table",
                    "instant": true,
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {},
                "overrides": [
                    {
                        "matcher": {
                            "id": "byName",
                            "options": "Value"
                        },
                        "properties": [
                            {
                                "id": "mappings",
                                "value": [
                                    {
                                        "type": "value",
                                        "options": {
                                            "-1": {
                                                "text": "ERROR",
                                                "color": "red"
                                            },
                                            "0": {
                                                "text": "STOPPED",
                                                "color": "orange"
                                            },
                                            "1": {
                                                "text": "RUNNING",
                                                "color": "green"
                                            }
                                        }
                                    }
                                ]
                            }
                        ]
                    }
                ]
            },
            "transformations": [
                {
                    "id": "organize",
                    "options": {
                        "excludeByName": {
                            "Time": true,
                            "__name__": true,
                            "instance": true,
                            "job": true
                        },
                        "renameByName": {
                            "integration_name": "Integration",
                            "Value": "Status"
                        }
                    }
                }
            ]
        },
        {
            "id": 8,
            "title": "Executions by Integration",
            "type": "timeseries",
            "gridPos": {
                "x": 8,
                "y": 4,
                "w": 8,
                "h": 6
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "rate(integration_executions_total[5m])",
                    "legendFormat": "{{integration_name}} ({{status}})",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "ops"
                }
            }
        },
        {
            "id": 9,
            "title": "Execution Duration (P95)",
            "type": "timeseries",
            "gridPos": {
                "x": 16,
                "y": 4,
                "w": 8,
                "h": 6
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, rate(integration_execution_duration_seconds_bucket[5m]))",
                    "legendFormat": "{{integration_name}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                }
            }
        },
        {
            "id": 10,
            "title": "API Calls by Target Service",
            "type": "piechart",
            "gridPos": {
                "x": 0,
                "y": 10,
                "w": 6,
                "h": 6
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "sum by (target_service) (integration_api_calls_total)",
                    "legendFormat": "{{target_service}}",
                    "refId": "A"
                }
            ]
        },
        {
            "id": 11,
            "title": "Errors by Type",
            "type": "piechart",
            "gridPos": {
                "x": 6,
                "y": 10,
                "w": 6,
                "h": 6
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "sum by (error_type) (integration_errors_total)",
                    "legendFormat": "{{error_type}}",
                    "refId": "A"
                }
            ]
        },
        {
            "id": 12,
            "title": "API Call Latency by Service",
            "type": "timeseries",
            "gridPos": {
                "x": 12,
                "y": 10,
                "w": 12,
                "h": 6
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "histogram_quantile(0.95, rate(integration_api_call_duration_seconds_bucket[5m]))",
                    "legendFormat": "{{target_service}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "s"
                }
            }
        },
        {
            "id": 13,
            "title": "Records Processed Over Time",
            "type": "timeseries",
            "gridPos": {
                "x": 0,
                "y": 16,
                "w": 12,
                "h": 6
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "rate(integration_records_processed_total[5m])",
                    "legendFormat": "{{integration_name}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "ops"
                }
            }
        },
        {
            "id": 14,
            "title": "Error Rate by Integration",
            "type": "timeseries",
            "gridPos": {
                "x": 12,
                "y": 16,
                "w": 12,
                "h": 6
            },
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "rate(integration_errors_total[5m])",
                    "legendFormat": "{{integration_name}} - {{error_type}}",
                    "refId": "A"
                }
            ],
            "fieldConfig": {
                "defaults": {
                    "unit": "ops",
                    "color": {
                        "mode": "palette-classic"
                    }
                }
            }
        }
    ],
    "schemaVersion": 38,
    "version": 1
}